{# List of all tags  #}
<nav class="">
{% for tag in collections.tagList %}
  {% set tagUrl %}/tags/{{ tag | slug }}/{% endset %}
  {% if loop.last %}
    <a href="{{ tagUrl | url }}" title="{{ tag }}" >{{ tag | replace("_", " ") }}</a>
  {% else %}
    <a href="{{ tagUrl | url }}" title="{{ tag }}">{{ tag | replace("_", " ") }}</a>,
  {% endif %}
{% endfor %}
</nav>
